package typechecker.model;

import typechecker.TypeVisitor;
import typechecker.Visitor;

public class Identifier {
	public String s;
	private int line;

	public Identifier(String as, int line) {
		s = as;
		this.line = line;
	}

	public void accept(Visitor v) {
		v.visit(this);
	}

	public Type accept(TypeVisitor v) {
		return v.visit(this);
	}

	public String toString() {
		return s;
	}
	
	public int getLine() {
		return line;
	}
}