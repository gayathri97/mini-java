package typechecker.model;

import typechecker.TypeVisitor;
import typechecker.Visitor;

public class NewArray extends Exp {
	public Exp e;

	public NewArray(Exp ae) {
		super(ae.line);
		e = ae;
	}

	public void accept(Visitor v) {
		v.visit(this);
	}

	public Type accept(TypeVisitor v) {
		return v.visit(this);
	}
	
	public String toString() {
		return "new int[" + e + "]";
	}
}
